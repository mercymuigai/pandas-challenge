    "purchase_count_spenders = top_spenders[\"Price\"].mean()\n",
    "\n",
    "#Average purchase by name\n",
    "\n",
    "Avg_purchase_price_spenders = top_spenders[\"Price\"].mean()\n",
    "\n",
    "# Total Purchase per spender\n",
    "\n",
    "total_purchase_spenders = top_spenders[\"Price\"].sum()\n",
    "\n",
    "\n",
    "#creating dataFrame\n",
    "\n",
    "Top_Spenders = pd.DataFrame({\"Purchase Count\":purchase_count_spenders, \n",
    "                \"Average Purchase Price\": Avg_purchase_price_spenders,\n",
    "                \"Total Purchase Value\":total_purchase_spenders})\n",
    "\n",
    "# Sorting and Formating\n",
    "\n",
    "spenders_formating = Top_Spenders.sort_values([\"Total Purchase Value\"],ascending= False).head()\n",
    "\n",
    "Top_Spenders[\"Average Purchase Price\"] = Top_Spenders[\"Average Purchase Price\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "Top_Spenders[\"Total Purchase Value\"] = Top_Spenders[\"Total Purchase Value\"].map(\"${:,.2f}\".format)\n",
    "                                              \n",
    "                                              \n",
    "Top_Spenders                                              "
   ]
