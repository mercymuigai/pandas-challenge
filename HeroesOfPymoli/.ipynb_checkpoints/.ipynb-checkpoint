    "#Creating bins for ages\n",
    "\n",
    "bins = [0,9, 14, 19, 24, 29, 34, 39, 100]\n",
    "\n",
    "# Labels for the bins\n",
    "age_ranges = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "\n",
    "my_total_number_of_players = purchase_data[\"SN\"].nunique()\n",
    "\n",
    "purchase_data[\"Age Group\"] = pd.cut(purchase_data[\"Age\"], bins, labels=age_ranges)\n",
    "purchase_data\n",
    "\n",
    "# Groupby Age Group\n",
    "age_groupby = purchase_data.groupby(\"Age Group\")\n",
    "\n",
    "\n",
    "\n",
    "# The total Count of Players based on SN\n",
    "total_count_age = age_groupby[\"SN\"].nunique()\n",
    "\n",
    "\n",
    "# Calculating Percentages of each age_range based on Age categoty of Purchased data\n",
    "\n",
    "Percentage_age =(total_count_age/total_players)*100\n",
    "\n",
    "\n",
    "#Creating A DataFrame\n",
    "Age_Demographics = pd.DataFrame({\"Total Count\":total_count_age,\"Percentage of Players\": Percentage_age })\n",
    "\n",
    "\n",
    "#Formating\n",
    "Age_Demographics.index.name = None\n",
    "\n",
    "Age_Demographics[\"Percentage of Players\"] = Age_Demographics[\"Percentage of Players\"].map(\"${:.2f}\".format)\n",
    "\n",
    "Age_Demographics"
