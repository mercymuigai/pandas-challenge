    "# Most Popular items\n",
    "\n",
    "#Retrieving Item ID, Item columns price and Item Name\n",
    "\n",
    "Items_Data = purchase_data[[\"Item ID\",\"Item Name\",\"Price\"]]\n",
    "\n",
    "#Grouping by Item ID and Item Name\n",
    "\n",
    "Items_Data_grouped = Items_Data.groupby([\"Item ID\", \"Item Name\"])\n",
    "\n",
    "# grouped Purchase count \n",
    "\n",
    "items_purchase_count = Items_Data_grouped[\"Price\"].count()\n",
    "                                \n",
    "\n",
    "#Purchase Value \n",
    "\n",
    "Total_Purchase_value = Items_Data_grouped[\"Price\"].sum()\n",
    "\n",
    "# Item Price\n",
    "\n",
    "Item_Price = Total_Purchase_value / items_purchase_count\n",
    "                                        \n",
    "# Average Purchase price per user\n",
    "                                        \n",
    "average_Purchase_price = Items_Data_grouped[\"Price\"].mean()\n",
    "                                    \n",
    "\n",
    "#Creating a Data Frame\n",
    "\n",
    "Popular_Items = pd.DataFrame({\"Purchase Count\":items_purchase_count,\n",
    "                              \"Item Price\":Item_Price,\n",
    "                              \"Total Purchase Value\":Total_Purchase_value})\n",
    "\n",
    "#Formating\n",
    "\n",
    "Popular_Items[\"Item Price\"]= Popular_Items[\"Item Price\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "Popular_Items[\"Total Purchase Value\"]= Popular_Items[\"Total Purchase Value\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "# Sorting in descending order\n",
    "\n",
    "sorting_popular_items = Popular_Items.sort_values(\"Purchase Count\", ascending = False)\n",
    "\n",
    "Popular_Items.head (5)\n"
   ]
